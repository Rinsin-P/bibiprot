{
  "version": 2,
  "builds": [
    {
      "src": "protfolio/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/static/favicon.png"
    },
    {
      "src": "/(.*)",
      "dest": "protfolio/wsgi.py"
    }
  ],
  "env": {
    "DEBUG": "False",
    "SECRET_KEY": "django-insecure-_1nyk^0hg5^tb#t53ch*w!c$!83lpo!am)8xdq=#5d6_yska&s"
  }
}

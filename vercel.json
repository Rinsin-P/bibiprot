{
  "version": 2,
  "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput",
  "outputDirectory": "staticfiles",
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/staticfiles/assets/img/favicon.png"
    },
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "protfolio/wsgi.py"
    }
  ],
  "env": {
    "DEBUG": "False",
    "SECRET_KEY": "django-insecure-_1nyk^0hg5^tb#t53ch*w!c$!83lpo!am)8xdq=#5d6_yska&s"
  }
}

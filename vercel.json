{
  "version": 2,
  "builds": [
    {
      "src": "protfolio/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/staticfiles/assets/img/favicon.png"
    },
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "protfolio/wsgi.py"
    }
  ],
  "env": {
    "DEBUG": "False",
    "SECRET_KEY": "django-insecure-_1nyk^0hg5^tb#t53ch*w!c$!83lpo!am)8xdq=#5d6_yska&s"
  }
}
